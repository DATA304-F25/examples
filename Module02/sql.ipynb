    "import sqlite3 as sql\n",
    "import pandas as pd\n",
    "\n",
    "# connect to same file-based DB\n",
    "connection = sql.connect(\"mydb.db\")\n",
    "\n",
    "df = pd.read_sql_query(\n",
    "    \"SELECT grade, COUNT(*) as count FROM students GROUP BY grade\",\n",
    "    connection\n",
    ")\n",
    "\n",
    "print(df)"
